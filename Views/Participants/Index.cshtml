@model IEnumerable<VolunteerInitiativesSystem.Models.Participant>

@{
    ViewData["Title"] = "Participants";
}

<h2>Participants</h2>

<p>
    <a asp-action="Create" class="btn btn-primary">Create New</a>
</p>

<table class="table">
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
        </tr>
}
    </tbody>
</table>